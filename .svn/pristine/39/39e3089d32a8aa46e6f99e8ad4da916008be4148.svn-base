body {
				background-color: #F2F2F2;
			}
			
			.wrap {
				width: 100%;
			}
			
			.title {
				width: 100%;
				position: relative;
				text-align: center;
				font-size: 16px;
				color: #333333;
				/*font-weight: 600;*/
				height: 40px;
				line-height: 40px;
				background-color: #F7F7F7;
			}
			
			.back {
				position: absolute;
				left: 20px;
				top: 14px;
				width: 10px;
				height: 15px;
			}
			.headInfor{
				width: 100%;
				text-align: center;
				margin-top: 10px;
				background-color: white;
			}
			.headInfor p:nth-child(1){
				padding-top: 20px;
				padding-bottom: 15px;
				font-size: 16px;
				color: #666666;
			}
			.headInfor p:nth-child(2){
				padding-bottom: 30px;
				font-size: 28px;
				color: #F95D18;
			}
			.content{
				width: 100%;
				margin-top: 10px;
				background-color: white;
			}
			.content p{
				padding: 16px 0 17px 16px;
				color: #333333;
				background-color: #F2F2F2;
			}
			.content ul li{
				width: 100%;
				overflow: hidden;
				line-height: 40px;
				height: 40px;
				border-bottom: 1px solid #E0E0E0;
				color: #666666;
				font-size: 16px;
			}
			.content ul li span:nth-child(1){
				display: inline-block;
				float: left;
				margin-left: 15px;
			}
			.content ul li span:nth-child(2){
				display: inline-block;
				float: right;
				margin-right: 15px;
			}