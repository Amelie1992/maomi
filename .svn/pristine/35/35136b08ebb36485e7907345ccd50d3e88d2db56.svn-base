.wrap {
				width: 100%;
			}
			
			.head {
				width: 100%;
				background-image: url(../../images/zctz-bg.png);
				background-size: 100% 100%;
				background-repeat: no-repeat;
				background-position: center;
				text-align: center;
			}
			
			.head p:nth-child(1) {
				color: white;
				font-size: 20px;
				padding: 40px 0 12px 0;
			}
			
			.head p:nth-child(2) {
				color: white;
				font-size: 14px;
				padding: 10px 0 50px 0;
			}
			
			.mid {
				width: 100%;
				text-align: center;
				padding: 30px 0;
			}
			
			.mid-inner {
				width: 100%;
				height: 30px;
				/*line-height: 30px;*/
			}
			
			
			
			.mid-inner div:nth-child(1) div {
				float: left;
				line-height: 30px;
				width: 33%;
			}
			
			.mid-inner div img {
				width: 24px;
				height: 24px;
			}
			.mid-inner div img:active{
				position: relative;
				top: 1px;
			}
			.zctz-btn {
				width: 100%;
				padding: 10px 0;
			}
			.zctz-btn p{
				padding: 10px 0;
				color: #757575;
			}
			 .zctz-btn img {
				width: 70%;
				margin: 0 auto; 
			}
			 .zctz-btn img:active{
			 	position: relative;
				top: 1px;
			 }
			.tips{
				padding: 10px;
				background: url(../../images/zctz-tips-bg.png) no-repeat  100% 100%;
			}
			.tips p:nth-child(1){
				padding: 10px 0 10px 0;
				font-size: 16px;
				color: #212121;
			}
			.tips p:nth-child(2){
				padding: 10px 0 10px 0;
				font-size: 14px;
				color: #888;
			}