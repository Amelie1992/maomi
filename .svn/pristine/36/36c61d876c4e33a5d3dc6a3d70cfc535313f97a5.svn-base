	input[type="button"],
			input[type="submit"],
			input[type="reset"] {
				-webkit-appearance: none;
			}
			
			textarea {
				-webkit-appearance: none;
			}
			
			input[type="button"] {
				border-radius: 0;
			}
			
			.wrap {
				width: 100%;
			}
			
			.banner {
				width: 100%;
				/*height: 225px;*/
			}
			
			.swiper-container {
				width: 100%;
				height: 100%;
			}
			
			.swiper-slide {
				text-align: center;
				font-size: 18px;
				background: #fff;
				/* Center slide text vertically */
				display: -webkit-box;
				display: -ms-flexbox;
				display: -webkit-flex;
				display: flex;
				-webkit-box-pack: center;
				-ms-flex-pack: center;
				-webkit-justify-content: center;
				justify-content: center;
				-webkit-box-align: center;
				-ms-flex-align: center;
				-webkit-align-items: center;
				align-items: center;
				width: 100%;
			}
			
			.swiper-slide img {
				width: 100%;
			}
			
			.yqhy {
				width: 100%;
				padding: 20px 0;
				border-bottom: 1px solid #EEEEEE;
			}
			
			.yqhy table img {
				width: 90px;
				height: 60px;
			}
			
			.yqhy table {
				width: 100%;
				text-align: center;
			}
			
			.guide {
				width: 100%;
				text-align: center;
				margin-top: 10px;
			}
			
			.guide img {
				width: 90%;
				text-align: center;
			}
			.att{
				text-align: left;	
				color:#f9bb4e;
				margin-left:7%;
				margin-bottom:10px;
				margin-top:10px;		
			}
			.att img{
				vertical-align:middle;
				width:16px;
				height:16px;				
			}
			.space {
				width: 100%;
				height: 15px;
				background-color: #F7F7F7;
			}
			
			.content-box {
				padding: 12px;
			}
			
			.content-box p:nth-child(1) {
				padding: 12px 0;
				border-bottom: 1px solid #E0E0E0;
			}
			
			.content-title {
				padding: 5px 10px;
				color: white;
				text-align: center;
			}
			
			.content-box p:nth-child(1) .content-title {
				background-size: 100% 100%;
				background-position: center;
				background-repeat: no-repeat;
			}
			
			.content-title-tips {
				float: right;
				font-size: 12px;
			}
			
			.content-box .xszx-title {
				background-image: url(../../images/fp-xszx.png);
			}
			
			.content-box .jxlc-title {
				background-image: url(../../images/fp-jxlc.png);
			}
			
			.content-box .rmtj-title {
				background-image: url(../../images/fp-rmtj.png);
			}
			
			.content-box .cryg-title {
				background-image: url(../../images/fp-cryg.png);
			}
			
			.content-box .tszc-title {
				background-image: url(../../images/fp-tszc.png);
			}
			
			.content-box .yfb-time {
				padding: 8px;
				background-color: #ffae07;
				border-radius: 2px;
				color: white;
			}
			.xszx-tips {
				color: #F95D18;
			}
			
			.jxlc-tips {
				color: #1ac6f8;
			}
			
			.rmtj-tips {
				color: #ff1818;
			}
			
			.cryg-tips {
				color: #28e931;
			}
			
			.tszc-tips {
				color: #e022f6;
			}
			
			.content-detail-title {
				padding: 15px 0;
				font-size: 14px;
				font-weight: bold;
				color: #212121;
			}
			.content-detail-title span{
				font-size: 12px;
				font-weight: normal;
				color: #212121;
				padding: 2px 3px;
				border: 1px solid #E0E0E0;
				border-radius: 10px;
				/*line-height: 10px;*/
				text-align: center;
			}
			.content-box table {
				width: 100%;
				text-align: left;
				margin-bottom: 10px;
			}
			
			.content-box table th {
				font-size: 12px;
				color: #757575;
			}
			
			.content-box table th span {
				color: #212121;
			}
			
			.content-box table th img {
				width: 90%;
				margin: 0 auto;
				text-align: center;
			}
			
			.content-box table th:nth-child(1) {
				width: 30%;
			}
			
			.content-box table th:nth-child(2) {
				width: 45%;
			}
			
			.content-box table th:nth-child(3) {
				width: 25%;
			}
			
			.content-box table td {
				color: #757575;
				font-size: 12px;
				line-height:30px;
			}
			
			.content-box table td span {
				color: #212121;
			}
			
			.content-box table th:nth-child(1) {
				color: #F95D18;
			}
			
			.content-box .content-rate {
				font-size: 24px;
				color: #F95D18;
				font-weight: bold;
			}
			
			.zc-box p {
				line-height: 22px;
				padding: 3px 0;
			}
			
			.content .zc-box table th {
				width: 50%;
			}
			
			.lookmore {
				text-align: center;
				border-top: 1px solid #E0E0E0;
				padding: 10px 0;
				color: #FFAD2C;
			}
			
			.currentChose_head {
				color: #FC7831;
				border-bottom: 2px solid #FC7831;
			}
			
			.currentChose {
				color: #FC7831;
			}
			
			.about {
				width: 100%;
				height: 70px;
				padding-top: 10px;
				background-color: #F7F7F7;
				margin-bottom: 50px;
			}
			
			.about table {
				width: 100%;
				text-align: center;
			}
			
			.about table img {
				width: 27px;
				height: 27px;
			}
			
			.footer {
				background-color: #fafafa;
				/*height: 80px;*/
				position: fixed;
				left: 0;
				bottom: 0;
				width: 100%;
				padding-top: 10px;
			}
			
			.footer table {
				width: 100%;
				text-align: center;
				font-size: 14px;
			}
			
			.footer table img {
				width: 30%;
			}
			
			.redPoint {
				display: inline-block;
				width: 8px;
				height: 8px;
				background-color: red;
				border-radius: 5px;
				position: absolute;
			}
			
			@media only screen and (min-height: 737px) and (max-height: 812px) {
				.about {
					width: 100%;
					height: 150px;
					padding-top: 10px;
					background-color: #F7F7F7;
				}
				.footer {
					background-color: #fafafa;
					padding-top: 10px;
					height: 87px;
					position: fixed;
					left: 0;
					bottom: 0;
					width: 100%;
				}
			}