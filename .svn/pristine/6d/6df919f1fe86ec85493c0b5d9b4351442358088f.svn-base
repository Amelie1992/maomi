* {
	margin: 0;
	padding: 0;
}

html,
body {
	/*background-color: whitesmoke;*/
	width: 100%;
	font-family: "微软雅黑";
}

a {
	text-decoration: none;
}

li {
	list-style: none;
}

.back {
	position: absolute;
	left: 20px;
	top: 18px;
	width: 10px;
	height: 15px;
}

#wrap p:nth-child(1) {
	font-size: 20px;
	color: #333333;
	background-color: #F7F7F7;
	text-align: center;
	padding: 10px;
	width: 100%;
}
/*去除ios下默认的按钮样式*/

input[type="button"],
input[type="submit"],
input[type="reset"] {
	-webkit-appearance: none;
}

textarea {
	-webkit-appearance: none;
}

input[type="button"] {
	border-radius: 0;
}