package com.xed.financing.wxgzh.model.param;

/**
 * @className:com.xed.financing.wxgzh.model.param.ParamBean
 * @description:
 * @version:v1.0.0 
 * @date:2017年3月21日 下午3:29:48
 * @author:Qian Tao
 */
public class ParamBean
{
	/**
	 * id
	 */
	private String paramId = "";

	/**
	 * 参数key
	 */
	private String paramKey = "";

	/**
	 * 参数值
	 */
	private String paramValue = "";

	/**
	 * 参数描述
	 */
	private String paramDescribe = "";

	/**
	 * 参数状态0：有效，1：无效,
	 */
	private String status = "";

	/**
	 * 添加时间
	 */
	private String addTime = "";
	
	/**
	 * 是否显示 
	 */
	private String isShow;

	/**
	 * 当前时间 精确到天
	 */
	private String nowDay;
	
	/**
	 * 当前时间  精确到秒
	 */
	private String nowHours;
	
	public String getNowDay()
	{
		return nowDay;
	}

	public void setNowDay(String nowDay)
	{
		this.nowDay = nowDay;
	}

	public String getNowHours()
	{
		return nowHours;
	}

	public void setNowHours(String nowHours)
	{
		this.nowHours = nowHours;
	}

	public String getParamId()
	{
		return paramId;
	}

	public void setParamId(String paramId)
	{
		this.paramId = paramId;
	}

	public String getParamKey()
	{
		return paramKey;
	}

	public void setParamKey(String paramKey)
	{
		this.paramKey = paramKey;
	}

	public String getParamValue()
	{
		return paramValue;
	}

	public void setParamValue(String paramValue)
	{
		this.paramValue = paramValue;
	}

	public String getParamDescribe()
	{
		return paramDescribe;
	}

	public void setParamDescribe(String paramDescribe)
	{
		this.paramDescribe = paramDescribe;
	}

	public String getStatus()
	{
		return status;
	}

	public void setStatus(String status)
	{
		this.status = status;
	}

	public String getAddTime()
	{
		return addTime;
	}

	public void setAddTime(String addTime)
	{
		this.addTime = addTime;
	}

	public String getIsShow()
	{
		return isShow;
	}

	public void setIsShow(String isShow)
	{
		this.isShow = isShow;
	}
	
}
