 * @author:Elias Zheng
 */
public class GoodsOrderBean
{
	/**
	 * 订单ID
	 */
	private String orderId;
	
	/**
	 * 帐户ID
	 */
	private String accountId;
	
	/**
	 * 商品ID
	 */
	private String goodsId;
	
	/**
	 * 支付方式(0:积分商城 1:爆款投标)
	 */
	private String orderType;
	
	/**
	 * 数量(支付方式为1时数量只能为1)
	 */
	private String goodsNum;
	
	/**
	 * 支付金额
	 */
	private String goodsMoney;
	
	/**
	 * 支付时间
	 */
	private String orderTime;
	
	/**
	 * 订单状态(0:交易关闭 1:已支付 2:已发货 3:已收货 4:已退款 5:退货)
	 */
	private String orderStatus;
	
	/**
	 * 收件人
	 */
	private String orderUserName;
	
	/**
	 * 联系电话
	 */
	private String orderUserTelephone;
	
	/**
	 * 收货地址
	 */
	private String orderUserAddr;
	
	/**
	 * 备注
	 */
	private String remark;
	
	/**
	 * 修改时间
	 */
	private String modTime;
	
	/**
	 * 修改时间
	 */
	private String goodsName;
	
	public String getOrderId()
	{
		return orderId;
	}

	public void setOrderId(String orderId)
	{
		this.orderId = orderId;
	}

	public String getAccountId()
	{
		return accountId;
	}

	public void setAccountId(String accountId)
	{
		this.accountId = accountId;
	}

	public String getGoodsId()
	{
		return goodsId;
	}

	public void setGoodsId(String goodsId)
	{
		this.goodsId = goodsId;
	}

	public String getOrderType()
	{
		return orderType;
	}

	public void setOrderType(String orderType)
	{
		this.orderType = orderType;
	}

	public String getGoodsNum()
	{
		return goodsNum;
	}

	public void setGoodsNum(String goodsNum)
	{
		this.goodsNum = goodsNum;
	}

	public String getGoodsMoney()
	{
		return goodsMoney;
	}

	public void setGoodsMoney(String goodsMoney)
	{
		this.goodsMoney = goodsMoney;
	}

	public String getOrderTime()
	{
		return orderTime;
	}

	public void setOrderTime(String orderTime)
	{
		this.orderTime = orderTime;
	}

	public String getOrderStatus()
	{
		return orderStatus;
	}

	public void setOrderStatus(String orderStatus)
	{
		this.orderStatus = orderStatus;
	}

	public String getOrderUserName()
	{
		return orderUserName;
	}

	public void setOrderUserName(String orderUserName)
	{
		this.orderUserName = orderUserName;
	}

	public String getOrderUserTelephone()
	{
		return orderUserTelephone;
	}

	public void setOrderUserTelephone(String orderUserTelephone)
	{
		this.orderUserTelephone = orderUserTelephone;
	}

	public String getOrderUserAddr()
	{
		return orderUserAddr;
	}

	public void setOrderUserAddr(String orderUserAddr)
	{
		this.orderUserAddr = orderUserAddr;
	}

	public String getRemark()
	{
		return remark;
	}

	public void setRemark(String remark)
	{
		this.remark = remark;
	}

	public String getModTime()
	{
		return modTime;
	}

	public void setModTime(String modTime)
	{
		this.modTime = modTime;
	}

	public String getGoodsName()
	{
		return goodsName;
	}

	public void setGoodsName(String goodsName)
	{
		this.goodsName = goodsName;
	}

	@Override
	public String toString()
	{
		return "GoodsOrder [orderId=" + orderId + ", accountId=" + accountId + ", goodsId=" + goodsId + ", orderType="
				+ orderType + ", goodsNum=" + goodsNum + ", goodsMoney=" + goodsMoney + ", orderTime=" + orderTime
				+ ", orderStatus=" + orderStatus + ", orderUserName=" + orderUserName + ", orderUserTelephone="
				+ orderUserTelephone + ", orderUserAddr=" + orderUserAddr + ", remark=" + remark + ", modTime="
				+ modTime + "]";
	}

	/**
	 * @param orderId
	 * @param accountId
	 * @param goodsId
	 * @param orderType
	 * @param goodsNum
	 * @param goodsMoney
	 * @param orderTime
	 * @param orderStatus
	 * @param orderUserName
	 * @param orderUserTelephone
	 * @param orderUserAddr
	 * @param remark
	 * @param modTime
	 */
	public GoodsOrderBean(String orderId, String accountId, String goodsId, String orderType, String goodsNum,
			String goodsMoney, String orderTime, String orderStatus, String orderUserName, String orderUserTelephone,
			String orderUserAddr, String remark, String modTime)
	{
		super();
		this.orderId = orderId;
		this.accountId = accountId;
		this.goodsId = goodsId;
		this.orderType = orderType;
		this.goodsNum = goodsNum;
		this.goodsMoney = goodsMoney;
		this.orderTime = orderTime;
		this.orderStatus = orderStatus;
		this.orderUserName = orderUserName;
		this.orderUserTelephone = orderUserTelephone;
		this.orderUserAddr = orderUserAddr;
		this.remark = remark;
		this.modTime = modTime;
	}

	/**
	 * @param orderId
	 * @param accountId
	 * @param goodsId
	 * @param orderType
	 * @param goodsNum
	 * @param goodsMoney
	 * @param orderTime
	 * @param orderStatus
	 * @param orderUserName
	 * @param orderUserTelephone
	 * @param orderUserAddr
	 * @param remark
	 * @param modTime
	 * @param goodsName
	 */
	public GoodsOrderBean(String orderId, String accountId, String goodsId, String orderType, String goodsNum,
			String goodsMoney, String orderTime, String orderStatus, String orderUserName, String orderUserTelephone,
			String orderUserAddr, String remark, String modTime, String goodsName)
	{
		super();
		this.orderId = orderId;
		this.accountId = accountId;
		this.goodsId = goodsId;
		this.orderType = orderType;
		this.goodsNum = goodsNum;
		this.goodsMoney = goodsMoney;
		this.orderTime = orderTime;
		this.orderStatus = orderStatus;
		this.orderUserName = orderUserName;
		this.orderUserTelephone = orderUserTelephone;
		this.orderUserAddr = orderUserAddr;
		this.remark = remark;
		this.modTime = modTime;
		this.goodsName = goodsName;
	}

	/**
	 * 
	 */
	public GoodsOrderBean()
	{
		super();
		// TODO Auto-generated constructor stub
	}
	
}
