.wrap {
				width: 100%;
				text-align: center;
				position: relative;
			}
			
			.wrap img {
				width: 100%;
			}
			
			.wx-double11-jxq-title:nth-of-type(1) {
				width: 70%;
				margin: 0 auto;
				position: absolute;
				left: 15%;
				top: 10.5%;
				font-size: 1rem;
				color: #212121;
			}
			
			.wx-double11-jxq-title:last-of-type {
				width: 80%;
				margin: 0 auto;
				position: absolute;
				left: 10%;
				top: 34.4%;
				font-size: 1rem;
				color: #212121;
			}
			
			.wx-double11-jxq-tips {
				width: 50%;
				position: absolute;
				right: 5%;
				color: white;
				font-size: 0.6rem;
				line-height: 1rem;
				text-align: left;
			}
			
			.wx-double11-jxq-tips:nth-of-type(2) {
				width: 50%;
				position: absolute;
				top: 13%;
				right: 5%;
				color: white;
				font-size: 0.8rem;
				line-height: 1rem;
				text-align: left;
			}
			
			.wx-double11-jxq-tips:nth-of-type(3) {
				width: 50%;
				top: 19.5%;
				color: #F95D18;
				text-align: center;
