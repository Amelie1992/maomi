body{
				background-color: #F2F2F2;
			}
			.wrap {
				width: 100%;
				min-height: 568px;
				
				
			}
			
			.content p {
				text-indent: 10px;
			}
			
			.title {
				width: 100%;
				position: relative;
				text-align: center;
				font-size: 18px;
				color: #333333;
				/*font-weight: 600;*/
				height: 40px;
				line-height: 40px;
				background-color: #F7F7F7;
			}
			
			.back {
				position: absolute;
				left: 20px;
				top: 14px;
				width: 10px;
				height: 15px;
			}
			
			.content {
				width: 100%;
				margin-top: 20px;
			}
			
			.content p {
				width: 100%;
				margin: 0 auto;
				height: 50px;
				background-color: white;
				line-height: 50px;
				font-size: 16px;
				color: #666666;
			}
			
			.content p:nth-child(1) {
				border-bottom: 1px solid #e0e0e0;
			}
			
			.content p:nth-child(1) input {
				width: 75%;
				border: none;
				outline: none;
				font-size: 14px;
				text-indent: 15px;
			}
			
			.content p:nth-child(2) input {
				margin-left: 15px;
			}
			
			.ktxje {
				height: 50px;
				line-height: 50px;
				font-size: 14px;
				padding-left: 10px;
			}
			
			.ktxje span {
				float: left;
				margin-right: 10px;
				color: #F95D18;
			}

			
			.wrap a {
				display: inline-block;
				width: 80%;
				margin-left: 10%;
				height: 40px;
				line-height: 40px;
				text-align: center;
				color: white;
				background-color: #FEB73B;
				border-radius: 5px;
				margin-top: 20px;
				margin-bottom: 20px;
			}
			.wrap a:active{
				position: relative;
				top: 1px;
			}
			.today-turnin
			{
				float:right;
				color:#888;
				padding-right:40px;
			}
			.today-turnin span
			{
				color:#F95D18;
			}
			.textbox {
				padding: 20px 10px;
				background-image: url(../../images/mmb-text-box.png);
				background-size: 95% 100%;
				background-position: center;
				background-repeat: no-repeat;
				
				background-color: #F2F2F2;
				margin-top:30px;
			}
			.textbox p{
				line-height: 25px;
				text-align: left;
				color: #888888;
				padding: 0 5px;
			}
			.textbox p:nth-of-type(1){
				color: #F95D18;
				font-size: 18px;
				
			}