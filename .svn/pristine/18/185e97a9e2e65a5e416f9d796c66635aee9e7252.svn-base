package com.xed.financing.wxgzh.model.usercapitaldetail;

/**
 * @className:com.xed.financing.wxgzh.model.usercapitaldetail.UserCapitalDetail
 * @description:
 * @version:v1.0.0
 * @date:2018年5月4日 下午2:53:21
 * @author:ZhangJun
 */
public class UserCapitalDetailBean
{
	/**
	 * 主键ID
	 */
	private String ID;

	/**
	 * 借款用户ID
	 */
	private String userId;

	/**
	 * 金额
	 */
	private String money;

	/**
	 * 交易类型
	 */
	private String type;

	/**
	 * 金额收支(0:收入 1:支出)
	 */
	private String inExpend;

	/**
	 * 交易时间
	 */
	private String dealTime;

	/**
	 * 说明
	 */
	private String remark;

	/**
	 * 是否显示(0:显示 1:不显示)
	 */
	private String isShow;

	public String getIsShow()
	{
		return isShow;
	}

	public void setIsShow(String isShow)
	{
		this.isShow = isShow;
	}

	public String getID()
	{
		return ID;
	}

	public void setID(String iD)
	{
		ID = iD;
	}

	public String getUserId()
	{
		return userId;
	}

	public void setUserId(String userId)
	{
		this.userId = userId;
	}

	public String getMoney()
	{
		return money;
	}

	public void setMoney(String money)
	{
		this.money = money;
	}

	public String getType()
	{
		return type;
	}

	public void setType(String type)
	{
		this.type = type;
	}

	public String getInExpend()
	{
		return inExpend;
	}

	public void setInExpend(String inExpend)
	{
		this.inExpend = inExpend;
	}

	public String getDealTime()
	{
		return dealTime;
	}

	public void setDealTime(String dealTime)
	{
		this.dealTime = dealTime;
	}

	public String getRemark()
	{
		return remark;
	}

	public void setRemark(String remark)
	{
		this.remark = remark;
	}

	public UserCapitalDetailBean()
	{
		super();
	}

	public UserCapitalDetailBean(String iD, String userId, String money, String type, String inExpend, String dealTime,
			String remark)
	{
		super();
		ID = iD;
		this.userId = userId;
		this.money = money;
		this.type = type;
		this.inExpend = inExpend;
		this.dealTime = dealTime;
		this.remark = remark;
	}

}
