<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="com.xed.financing.wxgzh.mapper.userlevel.UserLevelMapper">

	<update id="changeUserLevel" parameterType="accountInfo">
		UPDATE mm_account_level
		SET account_level = #{accountLevel}
		WHERE
			account_id = #{accountId}
	</update>
	
</mapper>